{
  "name": "skeleton",
  "version": "1.0.0",
  "description": "Starter template for weblab students",
  "scripts": {
    "hotloader": "webpack serve --config ./webpack.config.js --mode development --port 5000",
    "start": "TS_NODE_PROJECT='./server-tsconfig.json' nodemon --watch 'server/**/*.ts' --exec 'ts-node' server/server.ts",
    "start-windows": "set TS_NODE_PROJECT=server-tsconfig.json&& nodemon --watch \"server/**/*.ts*\" --watch \"server/**/*.js\" --exec \"ts-node\" server/server.ts",
    "build": "webpack --config ./webpack.config.js --mode production",
    "tunnel": "lt --port 5000 --subdomain sauce-thickens --local-host localhost"
  },
  "author": "johanc",
  "license": "MIT",
  "devDependencies": {
    "@types/reach__router": "^1.3.6",
    "@types/webpack": "^4.41.26",
    "@types/webpack-env": "^1.16.0",
    "css-loader": "^5.0.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.3.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@reach/router": "^1.3.4",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.10",
    "@types/express-session": "^1.17.3",
    "@types/mongoose": "^5.10.3",
    "@types/morgan": "^1.9.2",
    "antd": "^4.18.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "google-auth-library": "^6.1.4",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "socket.io": "^3.0.5",
    "socket.io-client": "^3.0.5",
    "ts-node": "^9.1.1",
    "twilio": "^3.73.1",
    "typescript": "^4.1.3"
  }
}
